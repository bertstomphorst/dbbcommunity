<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="prt_mail_message_search_dbb" model="ir.ui.view">
        <field name="name">prt.mail.message.search DBB</field>
        <field name="model">mail.message</field>
        <field name="inherit_id" ref="prt_mail_messages_advisto.prt_mail_message_search_advisto" />
        <field name="arch" type="xml">
            <filter name="filter_inbox_shared" position="before">
                <filter string="DBB Texel" name="filter_inbox_dbbtexel" domain="[('fetchmail_server_id', '=', 1)]"/>
                <filter string="Winkelhart van Texel" name="filter_inbox_winkelhartvantexel" domain="[('fetchmail_server_id', '=', 2)]"/>
                <separator/>
            </filter>
        </field>
    </record>
</odoo>
